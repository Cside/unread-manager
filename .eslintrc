{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "alert": true,
    "document": true,
    "__REDUX_DEVTOOLS_EXTENSION__": true
  },
  "plugins": [
    "import"
  ],
  "settings": {
    "import/parser": "babel-eslint"
  },
  "rules": {
    // Overlide rules of airbnb
    'no-multi-spaces': 0,
    'no-console': 0,
    'no-alert': 0,
    'arrow-body-style': 0,
    'no-param-reassign': 0,
    'key-spacing': 0,
    'arrow-parens': 0,
    'no-plusplus': 0,
    'react/prefer-stateless-function': 0,
    'import/no-dynamic-require': 0,
    'jsx-a11y/img-has-alt': 0,
    'no-script-url': 0
  }
}
